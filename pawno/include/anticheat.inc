forward accb_MainUpdate();
public accb_MainUpdate()
{
	for(new id = (ACc_WARN_LINE - 1); id >= 0; id--) if(warningPanelTime{id}) { if(--warningPanelTime{id} <= 0)
	{
	    warningPanelText[id] = "_";
		TextDrawSetString(warningPanelTD[id], "_");
	}}
	ACc_FORPLAYER(playerid)
	{
	    new Float:pos_x, Float:pos_y, Float:pos_z;
	    GetPlayerPos(playerid, pos_x, pos_y, pos_z);
		if(++accb_playerPaused[playerid] <= 3)
		{
		    for(new id = (ACc_NULL_WARN - 1); id >= 0; id--) if(playerNullWarningTimer[playerid][id]) { if(--playerNullWarningTimer[playerid][id] <= 0) switch(id)
		    {
		        case 0: if(--playerWarningSpeed{playerid} > 0) playerNullWarningTimer[playerid][id] = 10;
		        case 1: if(--playerWarningCarShot{playerid} > 0) playerNullWarningTimer[playerid][id] = 10;
		    }}
		    if(NetStats_MessagesRecvPerSecond(playerid) > 800) CheatDetected(playerid, ACc_RECVPER);
		    if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_USEJETPACK) CheatDetected(playerid, ACc_JETPACK);
		    
		    new ping = GetPlayerPing(playerid);
		    if(0 <= ping <= ACc_MAX_PING) playerPing[playerid] = ping;
		    if(ping > ACc_MAX_PING) { if(++playerWarningPing{playerid} > 10) CheatDetected(playerid, ACc_PING); }
			else playerWarningPing{playerid} = 0;
			
			if(playerClientCheck{playerid} <= 600) playerClientCheck{playerid}++;
			if(playerClientCheck{playerid} == 600) {}
			SendClientCheck(playerid, 0x2, 0, 0, 2);
			
			new gstate = GetPlayerState(playerid), vehid = GetPlayerVehicleID(playerid);
			if(gstate == PLAYER_STATE_DRIVER)
			{
				if(playerCheckRepair{playerid}) playerCheckRepair{playerid}--;
				else
				{
			    	new Float:heal;
			    	GetVehicleHealth(vehid, heal);
					if(!playerInModShop{playerid} && heal > vehicleHealth[vehid] && !IsPlayerInPayNSprayAC(playerid))
					{
					    SetVehicleHealth(vehid, vehicleHealth[vehid]);
						if(++playerWarningReapir{playerid} > 10) CheatDetected(playerid, ACc_REPAIR);
					}
					else
					{
		                vehicleHealth[vehid] = heal;
		                playerWarningReapir{playerid} = 0;
					}
				}
			}
		  	if(playerCheckWeapon{playerid}) playerCheckWeapon{playerid}--;
		    else { for(new id = 12; id >= 0; id--)
			{
			    new weap, ammo;
		 		GetPlayerWeaponData(playerid, id, weap, ammo);
		 		if(weap && weap != 1 && weap != 46)
		 		{
			 		if(ammo < accb_playerAmmo[playerid][id]) accb_playerAmmo[playerid][id] = ammo;
			        if(weap != accb_playerWeaponID[playerid][id]) CheatDetected(playerid, ACc_WEAPON);
	     		}
			}}
		  	if(playerCheckArmour{playerid}) playerCheckArmour{playerid}--;
			else
			{
				new Float:arm;
				GetPlayerArmour(playerid, arm);
				if(accb_playerArmour[playerid] < arm)
				{
				    SetPlayerArmour(playerid, accb_playerArmour[playerid]);
				    if(++playerWarningArmour{playerid} > 10) CheatDetected(playerid, ACc_ARMOUR);
				}
				else if(accb_playerArmour[playerid] > arm)
				{
					accb_playerArmour[playerid] = arm;
					playerWarningArmour{playerid} = 0;
				}
				else playerWarningArmour{playerid} = 0;
			}
			if(playerCheckHealth{playerid}) playerCheckHealth{playerid}--;
			else
			{
				new Float:heal;
				GetPlayerHealth(playerid, heal);
				if(accb_playerHealth[playerid] < heal)
				{
				    SetPlayerHealth(playerid, accb_playerHealth[playerid]);
				    if(++playerWarningHealth{playerid} > 10) CheatDetected(playerid, ACc_HEALTH);
				}
				else if(accb_playerHealth[playerid] > heal)
				{
					accb_playerHealth[playerid] = heal;
					playerWarningHealth{playerid} = 0;
				}
				else playerWarningHealth{playerid} = 0;
			}
			if(vehid) { if(playerRemoveVehicle{playerid}) { if(--playerRemoveVehicle{playerid} <= 0) CheatDetected(playerid, ACc_VEHREMOVE); }}
			else
			{
			    new animid = GetPlayerAnimationIndex(playerid);
			    switch(animid)
                {
					case 1058: CheatDetected(playerid, ACc_FLY4);
			    	case 157, 159, 161: CheatDetected(playerid, ACc_FLY1);
 				}
			    if((1538 <= animid <= 1544) && !IsPlayerInWater1AC(playerid)) { if(++playerWarningFly1{playerid} > 2) CheatDetected(playerid, ACc_FLY2); }
			    else playerWarningFly1{playerid} = 0;
			    if((animid == 958 || animid == 959) && GetPlayerWeapon(playerid) != 46) { if(++playerWarningFly2{playerid} > 2) CheatDetected(playerid, ACc_FLY3); }
                else playerWarningFly2{playerid} = 0;
			}
			new objid = GetPlayerSurfingObjectID(playerid);
			if(!objid || objid == 0xFFFF) playerWarningInviz{playerid} = 0;
			else
			{
				new Float:pos_ox, Float:pos_oy, Float:pos_oz;
				GetObjectPos(objid, pos_ox, pos_oy, pos_oz);
				if(!IsPlayerInRangeOfPoint(playerid, 50.0, pos_ox, pos_oy, pos_oz)) { if(++playerWarningInviz{playerid} > 10) CheatDetected(playerid, ACc_INVIZ0); }
				else playerWarningInviz{playerid} = 0;
			}
			if(playerCheckFly{playerid}) playerCheckFly{playerid}--;
		 	else
		 	{
			 	if((gstate == PLAYER_STATE_DRIVER || gstate == PLAYER_STATE_ONFOOT) && GetTickDiffAC(GetTickCount(), playerLagFly[playerid]) < 1500 && accb_playerPosZ[playerid] > -35.0)
			 	{
					new Float:dist = VectorSize(floatabs(accb_playerPosX[playerid] - pos_x), floatabs(accb_playerPosY[playerid] - pos_y), 0.0);
					if(gstate == PLAYER_STATE_DRIVER && dist > 200.0) CheatDetected(playerid, ACc_TP0);
					if(gstate == PLAYER_STATE_ONFOOT && dist > 60.0 && objid == 0xFFFF && GetPlayerSurfingVehicleID(playerid) == 0xFFFF) CheatDetected(playerid, ACc_TP1);//
					if(dist > 5.0 && GetPlayerSpeedAC(playerid) < 2) { if(++playerWarningFly0{playerid} > 2) CheatDetected(playerid, ACc_FLY0); }
					else playerWarningFly0{playerid} = 0;
				}
			}
		}
		playerLagFly[playerid] = GetTickCount();
	 	accb_playerPosX[playerid] = pos_x;
	 	accb_playerPosY[playerid] = pos_y;
	 	accb_playerPosZ[playerid] = pos_z;
	}
}

